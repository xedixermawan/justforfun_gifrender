        <Category Name="Command Line" Disp="Command Line">
          <Option Name="Additional options" Disp="Additional options" Description="Specify additional options." />
        </Category>
      </Tool>
      <Tool Name="Assembler" Deny="VertexShader">
        <Category Name="C Preprocessor" Disp="C Preprocessor">
