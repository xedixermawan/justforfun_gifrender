<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE Project[
  <!ELEMENT Project (OptionList,Localize?,OptionValues,DefaultValues?)>
  <!ATTLIST Project
    Platform CDATA #REQUIRED
    Version CDATA #REQUIRED
    Type CDATA #REQUIRED
  >
  <!ELEMENT OptionList (Tool*)>
  <!ELEMENT Tool (Category|Option)*>
  <!ATTLIST Tool
    Name CDATA #REQUIRED
    Disp CDATA #IMPLIED
    Allow CDATA #IMPLIED
    Deny CDATA #IMPLIED
  >
  <!ELEMENT Category (Option*)>
  <!ATTLIST Category
    Name CDATA #REQUIRED
    Disp CDATA #IMPLIED
    Allow CDATA #IMPLIED
    Deny CDATA #IMPLIED
  >
  <!ELEMENT Option ANY>
  <!ATTLIST Option
    Name CDATA #REQUIRED
    Disp CDATA #IMPLIED
    Description CDATA #IMPLIED
    Type (Text|SplitText|MultiText|List|Select|MultiSelect) #IMPLIED
    Format CDATA #IMPLIED
    AllowEmpty (true|false) #IMPLIED
    Separator CDATA #IMPLIED
    Browse (None|File|Folder) #IMPLIED
    Regexp CDATA #IMPLIED
    Allow CDATA #IMPLIED
    Deny CDATA #IMPLIED
  >
  <!ELEMENT Item (#PCDATA)>
  <!ATTLIST Item
    Name CDATA #IMPLIED
    Disp CDATA #IMPLIED
    Allow CDATA #IMPLIED
    Deny CDATA #IMPLIED
  >
  <!ELEMENT Localize (Local*)>
  <!ELEMENT Local (Tool*)>
  <!ATTLIST Local Name CDATA #REQUIRED>
  <!ELEMENT OptionValues (Build*)>
  <!ELEMENT Build (Tool*)>
  <!ATTLIST Build
    Name CDATA #REQUIRED
    Allow CDATA #IMPLIED
    Deny CDATA #IMPLIED
  >
  <!ELEMENT DefaultValues (Tool*)>
]>
<Project Platform="CTR" Version="1.0.1.0" Type="Library">
  <OptionList>
    <Tool Name="General">
      <Category Name="General">
        <Option Name="Access path" Type="List" Regexp="^[_0-9a-zA-Z]+=.*$" />
        <Option Name="Code generation" Type="Select" Deny="VertexShader">
          <Item Name="fast">--arm -DNN_EFFORT_FAST -DNN_COMPILER_OPTION_ARM</Item>
          <Item Name="small">--thumb -DNN_EFFORT_SMALL</Item>
        </Option>
        <Option Name="Optimize type" Type="Select" Deny="VertexShader">
          <Item Name="fast">-Otime</Item>
          <Item Name="small">-Ospace</Item>
        </Option>
      </Category>
    </Tool>
    <Tool Name="Compiler" Deny="VertexShader">
      <Category Name="General">
        <Option Name="Addtional include path" Type="MultiText" Format="-I&quot;${value}&quot;" Separator=";" Browse="Folder" />
      </Category>
      <Category Name="Optimize">
        <Option Name="Optiomze level" Type="Select" Format="-O${value}">
          <Item Name="0">0</Item>
          <Item Name="1">1</Item>
          <Item Name="2">2</Item>
          <Item Name="3">3</Item>
        </Option>
        <Option Name="Optimize retain" Type="Select">
          <Item Name="fns">--retain=fns</Item>
          <Item Name="inlinefns">--retain=inlinefns</Item>
          <Item Name="noninlinefns">--retain=noninlinefns</Item>
          <Item Name="paths">--retain=paths</Item>
          <Item Name="calls">--retain=calls</Item>
          <Item Name="calls:distinct">--retain=calls:distinct</Item>
          <Item Name="libcalls">--retain=libcalls</Item>
          <Item Name="data">--retain=data</Item>
          <Item Name="rodata">--retain=rodata</Item>
          <Item Name="rwdata">--retain=rwdata</Item>
          <Item Name="data:order">--retain=data:order</Item>
          <Item Name="none">
          </Item>
        </Option>
        <Option Name="Optimize type" Type="Text" />
      </Category>
      <Category Name="Pre procerssor">
        <Option Name="Pre procerssor" Type="MultiText" Format="-D&quot;${value}&quot;" Separator=";" />
        <Option Name="Disable Pre-procerssor symbol" Type="MultiText" Format="-U&quot;${value}&quot;" Separator=";" />
        <Option Name="Pre include file" Type="MultiText" Format="--preinclude=&quot;${value}&quot;" Browse="File" Separator=";" />
      </Category>
      <Category Name="Code genaration">
        <Option Name="Support interwork" Type="Select">
          <Item Name="True">--apcs=/interwork</Item>
          <Item Name="False">--apcs=/nointerwork</Item>
        </Option>
        <Option Name="CPU" Type="Text" Format="--cpu ${value}" />
        <Option Name="Split ELF sections" Type="Select">
          <Item Name="True">--split_sections</Item>
          <Item Name="False">
          </Item>
        </Option>
        <Option Name="Endian" Type="Select">
          <Item Name="big">--bigend</Item>
          <Item Name="little">--littleend</Item>
        </Option>
      </Category>
      <Category Name="Language">
        <Option Name="Enable exception" Type="Select">
          <Item Name="True">--exceptions</Item>
          <Item Name="False">--no_exceptions</Item>
        </Option>
        <Option Name="Force new expr" Type="Select">
          <Item Name="True">--force_new_nothrow</Item>
          <Item Name="False">
          </Item>
        </Option>
        <Option Name="GNU extensions" Type="Select">
          <Item Name="True">--gnu</Item>
          <Item Name="False">
          </Item>
        </Option>
        <Option Name="Multi byte characters" Type="Select">
          <Item Name="True">--multibyte_chars</Item>
          <Item Name="False">
          </Item>
        </Option>
        <Option Name="Support RTTI" Type="Select">
          <Item Name="True">--rtti</Item>
          <Item Name="False">--no_rtti</Item>
        </Option>
        <Option Name="[char] is signed" Type="Select">
          <Item Name="True">--signed_chars</Item>
          <Item Name="False">--unsigned_chars</Item>
        </Option>
        <Option Name="Enable delete vfunc" Type="Select">
          <Item Name="True">--vfe</Item>
          <Item Name="False">--no_vfe</Item>
        </Option>
        <Option Name="[wchar_t] is ushort" Type="Select">
          <Item Name="True">--wchar16</Item>
          <Item Name="False">--wchar32</Item>
        </Option>
      </Category>
      <Category Name="Pre compile">
        <Option Name="Pre compile" Type="Select">
          <Item Name="True">--pch</Item>
          <Item Name="False">
          </Item>
        </Option>
        <Option Name="PCH directory" Type="Text" Format="--pch_dir=&quot;${value}&quot;" Browse="Folder" />
      </Category>
      <Category Name="Debug">
        <Option Name="Create debug table" Type="Select">
          <Item Name="True">--debug</Item>
          <Item Name="False">
          </Item>
        </Option>
        <Option Name="Create debug macro table" Type="Select">
          <Item Name="True">--debug_macros</Item>
          <Item Name="False">--no_debug_macros</Item>
        </Option>
        <Option Name="DWARF2" Type="Select">
          <Item Name="DWARF2">--dwarf2</Item>
          <Item Name="DWARF3">--dwarf3</Item>
        </Option>
        <Option Name="Remove Unneeded Symbol" Type="Select">
          <Item Name="True">--remove_unneeded_entities</Item>
          <Item Name="False">--no_remove_unneeded_entities</Item>
        </Option>
      </Category>
      <Category Name="Message">
        <Option Name="Disable diag error message" Type="SplitText" Format="--diag_error ${value}" Separator="," />
        <Option Name="Message style" Type="Select" Format="--diag_style=${value}">
          <Item Name="arm">arm</Item>
          <Item Name="ide">ide</Item>
          <Item Name="gnu">gnu</Item>
        </Option>
        <Option Name="Disable diag suppress message" Type="SplitText" Format="--diag_suppress ${value}" Separator="," />
        <Option Name="Disable diag warning message" Type="SplitText" Format="--diag_warning ${value}" Separator="," />
        <Option Name="Output remark message" Type="Select">
          <Item Name="True">--remarks</Item>
          <Item Name="False">
          </Item>
        </Option>
      </Category>
      <Category Name="Detail">
        <Option Name="Data reorder" Type="Select">
          <Item Name="True">--data_reorder</Item>
          <Item Name="False">--no_data_reorder</Item>
        </Option>
        <Option Name="Floating point mode" Type="Select" Format="--fpmode=${value}">
          <Item Name="ieee_full">ieee_full</Item>
          <Item Name="ieee_fixed">ieee_fixed</Item>
          <Item Name="ieee_no_fenv">ieee_no_fenv</Item>
          <Item Name="fast">fast</Item>
        </Option>
        <Option Name="Unaligned access" Type="Select">
          <Item Name="True">
          </Item>
          <Item Name="False">--no_unaligned_access</Item>
        </Option>
      </Category>
      <Category Name="Command Line">
        <Option Name="Additional options" Type="Text" />
      </Category>
    </Tool>
    <Tool Name="Assembler" Deny="VertexShader">
      <Category Name="C Preprocessor">
        <Option Name="Using C Preprocessor" Type="Select">
          <Item Name="True">-A"--cpreproc"</Item>
          <Item Name="False">
          </Item>
        </Option>
        <Option Name="Preprocerssor" Type="MultiText" Format="-D&quot;${value}&quot;" Separator=";">
        </Option>
        <Option Name="Include path" Type="MultiText" Format="-I&quot;${value}&quot;" Separator=";" Browse="Folder">
        </Option>
        <Option Name="Disable Preprocessor symbol" Type="MultiText" Format="-U&quot;${value}&quot;" Separator=";">
        </Option>
      </Category>
      <Category Name="Advanced">
        <Option Name="Assembler Include path" Type="SplitText" Format="-A&quot;-i&quot;${value}&quot;&quot;" Separator="," Browse="Folder">
        </Option>
        <Option Name="Additional options" Type="MultiText" Format="-A&quot;${value}&quot;" Separator="," />
      </Category>
    </Tool>
    <Tool Name="Librarian" Allow="Library">
      <Category Name="General">
        <Option Name="Output file" Type="Text" Format="&quot;${value}&quot;" Browse="File" />
        <Option Name="Archive option" Type="Text">
        </Option>
      </Category>
      <Category Name="Message">
        <Option Name="Message style" Type="Select" Format="--diag_style=${value}">
          <Item Name="arm">arm</Item>
          <Item Name="ide">ide</Item>
          <Item Name="gnu">gnu</Item>
        </Option>
      </Category>
      <Category Name="Command Line">
        <Option Name="Additional options" Type="Text">
        </Option>
      </Category>
    </Tool>
  </OptionList>
  <Localize>
    <Local Name="eng">
      <Tool Name="General">
        <Category Name="General" Disp="General">
          <Option Name="Access path" Disp="Reserved Name" Description="Set reserved name to use in property. Once the name is set to Reserved Name, when ${reserved name} is used in a property it will be replaced with the defined value." />
          <Option Name="Code generation" Disp="Type of Code Generation" Description="Specify designated code generation option to compiler.&#xA;(fast:--arm, small:--thumb)" />
          <Option Name="Library type" Disp="Type of Library" Description="Specify designated library to linker. &#xA;(fast:XXXX.fast.{a|o}, small:XXXX.small.{a|o})" />
          <Option Name="Optimize type" Disp="Optimazation Type" Description="Specify designated optimize option to compiler.&#xA;(fast:-Otime, small:-Ospace)" />
        </Category>
      </Tool>
      <Tool Name="Compiler" Disp="Compiler" Deny="VertexShader">
        <Category Name="General" Disp="General">
          <Option Name="Addtional include path" Disp="Additional Include Directories" Description="Specifies one or more directories to add to the include path. Visual Studio Macro can be used. (Example:$(SolutionDir)\include). Use separator ';' to specify more than one. &#xA;(-Ipath)" />
        </Category>
        <Category Name="Optimize" Disp="Optimization">
          <Option Name="Optiomze level" Disp="Optimization Level" Description="Specify optimization level &#xA;(-O{0|1|2|3})" />
          <Option Name="Optimize retain" Disp="Restriction of optimization" Description="Restrict optimization &#xA;(--retain={fns|inlinefns|noninlinefns|paths|calls|calls:distinct|libcalls|data|rodata|rwdata|data:order})" />
          <Option Name="Optimize type" Disp="Optimazation Type" Description="Specify the reservation name ${OptimizeType} replaced to optimization option specified by [General]-[Optimization Type]. &#xA;Please do not edit ${OptimizeType}." />
        </Category>
        <Category Name="Pre procerssor" Disp="Preprocessor">
          <Option Name="Pre procerssor" Disp="Preprocessor Definitions" Description="Specifies one or more preprocessor defines. Use separator ';' to specify more than one. &#xA;(-Dname)" />
          <Option Name="Disable Pre-procerssor symbol" Disp="Undefine Preprocessor Definitions" Description="Specifies one or more preprocessor undefines. Use separator ';' to specify more than one. &#xA;(-Uname)" />
          <Option Name="Pre include file" Disp="Pre-include File" Description="Include source code of the specified file when compiling. Use separator ';' to specify more than one. &#xA;(--preinclude=file)" />
        </Category>
        <Category Name="Code genaration" Disp="Code Generation">
          <Option Name="Support interwork" Disp="Generation of Interworking Support Code" Description="Generate interworking support code. &#xA;(True:--apcs=/interwork, False:--apcs=/nointerwork)" />
          <Option Name="CPU" Disp="CPU" Description="Generate code for the specified CPU. &#xA;(--cpu MPCore)" />
          <Option Name="Split ELF sections" Disp="Generation of ELF selection" Description="Generate ELF section for each function in the source file. &#xA;(True:--split_sections, False:)" />
          <Option Name="Endian" Disp="Endian" Description="Specify the endianness to use when generating code. &#xA;(big:--bigend, little:--littleend)" />
        </Category>
        <Category Name="Language" Disp="Language">
          <Option Name="Enable exception" Disp="Exception Handling" Description="Enable exception handling &#xA;(True:--exceptions, False:--no_exceptions)" />
          <Option Name="Force new expr" Disp="Control new Expressions" Description="Control the behavior of new expressions in C++. &#xA;(True:--force_new_nothrow, False:)" />
          <Option Name="GNU extensions" Disp="GNU Extensions" Description="Enable GNU extensions. &#xA;(True:--gnu, False:)" />
          <Option Name="Multi byte characters" Disp="Usage of Multibyte Character" Description="Use multibyte character. &#xA;(True:--multibyte_chars, False:)" />
          <Option Name="Support RTTI" Disp="RTTI Feature" Description="Support RTTI feature dynamic_cast and typeid with C++. &#xA;(True:--rtti, False:--no_rtti)" />
          <Option Name="[char] is signed" Disp="Default to signed char" Description="Sets whether char defaults to a signed or unsigned type. &#xA;(True:--signed_chars, False:--unsigned_chars)" />
          <Option Name="Enable delete vfunc" Disp="C++ Virtual Function Elimination" Description="Enable Virtual Function Elimination in C++. &#xA;(True:--vfe, False:--no_vfe)" />
          <Option Name="[wchar_t] is ushort" Disp="Default to Unsigned Short for wchar_t" Description="Set the default wchar_t type to unsigned short. &#xA;(True:--wchar16, False:--wchar32)" />
        </Category>
        <Category Name="Pre compile" Disp="Precompiled Headers">
          <Option Name="Pre compile" Disp="Create/Use Precompiled Header" Description="Enables the creation and use of a precompiled header during the bulid. &#xA;(True:--pch, False:)" />
          <Option Name="PCH directory" Disp="Path of Precompiled Header" Description="Specify path for precompiled header. &#xA;(True:--pch_dir=path)" />
        </Category>
        <Category Name="Debug" Disp="Debugging">
          <Option Name="Create debug table" Disp="Generation of Debug Table" Description="Generate debug table. &#xA;(True:--debug, False:)" />
          <Option Name="Create debug macro table" Disp="Generation of Debug Table for Preprocessor" Description="Generate debug table entries for preprocessor macro definitions. &#xA;(True:--debug_macros, False:--no_debug_macros)" />
          <Option Name="DWARF2" Disp="Debug Table Format" Description="Specify debug table format. &#xA;(DWARF2:--dwarf2, DWARF3:--dwarf3)" />
          <Option Name="Remove Unneeded Symbol" Disp="Remove Unused Symbols" Description="Remove unused symbols from debug information. &#xA;(True:--remove_unneeded_entities, False:--no_remove_unneeded_entities)" />
        </Category>
        <Category Name="Message" Disp="Message">
          <Option Name="Disable diag error message" Disp="Setting Error Severity" Description="Set the diagnostic messages that have the specified numbers to Error severity. Use separator ',' to specify more than one. &#xA;(--diag_error num[,num...])" />
          <Option Name="Message style" Disp="Display Style of Diagnostic Message" Description="Specify display style of diagnostic message. &#xA;(--diag_style={arm|ide|gnu})" />
          <Option Name="Disable diag suppress message" Disp="Disable Diagnostic Messages" Description="Disable diagnostic messages that have the specified numbers. Use separator ',' to specify more than one.&#xA;(--diag_suppress num[,num...])" />
          <Option Name="Disable diag warning message" Disp="Setting Warning Severity" Description="Set the diagnostic message that have specified numbers to Warning severity. Use separator ',' to specify more than one. &#xA;(--diag_warning num[,num...])" />
          <Option Name="Output remark message" Disp="Remarks" Description="Display remark of warnings. &#xA;(True:--remarks, False:) " />
        </Category>
        <Category Name="Detail" Disp="Advanced">
          <Option Name="Data reorder" Disp="Automatic Reordering of Data Items" Description="Reordering of top-level data items automatically. &#xA;(True:--data_reorder, False:--no_data_reorder)" />
