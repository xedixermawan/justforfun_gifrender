      </Tool>
      <Tool Name="Librarian" Disp="Librarian" Allow="Library">
        <Category Name="General" Disp="General">
          <Option Name="Output file" Disp="Output File" Description="Specify output file." />
          <Option Name="Archive option" Disp="Archive options" Description="Specify archive option." />
        </Category>
        <Category Name="Message" Disp="Message">
          <Option Name="Message style" Disp="Display Style of Diagnostic Message" Description="Specify display style of diagnostic message. &#xA;(--diag_style={arm|ide|gnu})" />
        </Category>
        <Category Name="Command Line" Disp="Command Line">
          <Option Name="Additional options" Disp="Additional options" Description="Specify additional options." />
        </Category>
      </Tool>
